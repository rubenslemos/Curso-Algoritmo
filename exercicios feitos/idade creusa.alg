algoritmo "semnome"
// Função :
// Autor :
// Data : 17/05/2021
// Seção de Declarações 
var
   anoatual, anonas, idade: Inteiro
inicio
// Seção de Comandos
   Escreva("Em que ano você nasceu? ")
   Leia(anonas)
   Escreva("Em que ano estamos agora? ")
   Leia(anoatual)
   idade <- anoatual - anonas
   Escreva("Então você hoje tem", idade, " anos")
fimalgoritmo
