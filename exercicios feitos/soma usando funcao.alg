// Seção de Declarações
var
v1, v2, S : inteiro
funcao Soma (X, Y :inteiro) : inteiro
inicio
  retorne X+Y
fimfuncao
inicio
Escreva("Digite o primeiro valor: ")
leia(v1)
Escreva("Digite o segundo valor: ")
leia(v2)
S <- Soma(V1, V2)
Escreva(V1, " +", V2, " =",S)
fimalgoritmo
